package in.scalive.votezy.dto;

import lombok.AllArgsConstructor;
import lombok.Data;
import lombok.NoArgsConstructor;

@Data
@AllArgsConstructor
@NoArgsConstructor
public class VoteRequestDTO {
    @NotNull("VoterId is Required")
	Long voterId;
    @NotNull("CandidateId is Required")
    Long candidateId;
}
